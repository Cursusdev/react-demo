{
  "name": "react-demo",
  "version": "0.1.0",
  "description": "Site de d√©monstration front-end avec React",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "dev": "rm -rf dist && set NODE_ENV=development && webpack serve --mode development --config ./webpack.dev.js",
    "push": "rm -rf dist && yarn build && docker build -f Dockerfile -t react-demo . && docker run --name react-demo --rm -d -p 127.0.0.1:8080:80/tcp react-demo:latest",
    "kill": "docker container rm -f $(docker ps --filter ancestor=react-demo:latest -q) && docker rmi react-demo",
    "lint": "eslint src/**/*.js",
    "test": "jest",
    "build": "rm -rf dist && set NODE_ENV=production && webpack --mode production --config ./webpack.prod.js",
    "deploy": "yarn build && firebase deploy"
  },
  "dependencies": {
    "firebase": "^8.2.9",
    "history": "^5.0.0",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.2.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-tagcloud": "^2.1.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "copy-webpack-plugin": "^8.0.0",
    "core-js": "^3.10.1",
    "dotenv-webpack": "^7.0.1",
    "eslint": "^7.23.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.23.1",
    "express": "^4.17.1",
    "favicons": "^6.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "firebase-tools": "^9.6.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^5.2.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.32.8",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^6.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/dist/",
      "!<rootDir>/public/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
